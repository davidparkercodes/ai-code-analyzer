OPENAI_HIGH_MODEL=gpt-4-turbo

# Mistral models
MISTRAL_LOW_MODEL=mistral-tiny
MISTRAL_MEDIUM_MODEL=mistral-small
MISTRAL_HIGH_MODEL=mistral-large